(defn gen
    (d)
  (defn gf xs
    (let (v (apply d xs))
      (apply (->> comb-meta gf :generic v) xs)))
  (comb-meta! gf {:generic {}})
  gf)


(defx defg
    e (n d)
  (eval e
    (list def n (list gen d))))


(defx defm
    e (g d as . xs)
  (letr ((g d f) (map (part eval e) (list g d (list* fn as xs)))
         m (comb-meta g)
         r (m :generic))
      (comb-meta! g (assoc m :generic (assoc r d f)))))
