(defn nat (v)
  (rec nat (j 0)
    (v j)
    (nat (inc j))))


(defn foo (f)
  (f 1 2 3)
  (f 4 5 6)
  (f 7 8 9)
  :nil)


(defn bar (f)
  (f 10)
  (f 2)
  (f 10)
  (f 4)
  (f 5)
  (f 5)
  (f 5)
  (f 5)
  (f 5)
  (f 4)
  :nil)

(defn fibv
    (v)
  (rec fib (i 0 j 1)
    (v j)
    (fib j (+ i j))))


(test (-> nat
          (s/reject #(<= it 100))
          (s/map inc)
          (s/drop 4)
          (s/take 10)
          (s/drop 2)
          (s/iter + 0)
          (s/drop-while #(< it 500))
          (s/take-while #(< it 800))
          (s/list))
      '(550 663 777))

(test (s/list foo) '(1 2 3 4 5 6 7 8 9))


(test (-> bar s/dedup s/list) (list 10 2 10 4 5 4))


(test (-> nat (s/take 10) (s/every 3) (s/map +) (s/list)) '(3 12 21))

(test (-> fibv (s/take 35) (s/last)) '(9227465))
